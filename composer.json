{
	"description": "Nette addons portal",
	"keywords": ["nette", "components", "addons", "extensions"],
	"homepage": "https://componette.com",
	"type": "project",
	"license": ["BSD-3-Clause"],
	"authors": [
		{
			"name": "Milan Felix Sulc",
			"homepage": "https://jfx.cz"
		},
		{
			"name": "Componette Contributors",
			"homepage": "https://github.com/componette/componette.com/graphs/contributors"
		}
	],
	"require": {
		"php": ">= 5.6.0",
		"ext-curl": "*",
		"ext-mbstring": "*",
		"ext-mysqli": "*",
		"nette/application": "dev-master",
		"nette/bootstrap": "~2.4.0",
		"nette/caching": "dev-master",
		"nette/di": "~2.4.0",
		"nette/forms": "~2.4.0",
		"nette/http": "~2.4.0",
		"nette/robot-loader": "~2.4.0",
		"nette/security": "~2.4.0",
		"nette/utils": "~2.4.0",
		"latte/latte": "dev-master",
		"tracy/tracy": "~2.4.0",
		"nextras/orm": "~2.0.0",
		"nextras/dbal": "~2.0.0"
	},
	"require-dev": {
		"nette/tester": "~2.0.0",
		"jakub-onderka/php-parallel-lint": "~0.9.0",
		"squizlabs/php_codesniffer": "~2.5.0"
	},
	"autoload-dev": {
		"classmap": ["app/", "tests/"],
		"files": ["app/shortcuts.php"]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
